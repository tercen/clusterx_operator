{
  "name": "ClusterX",
  "description": "Clusters data points using an algorithm that finds density peaks.",
  "tags": [
    "clustering"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/clusterx_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "dimReduction",
      "defaultValue": "NULL",
      "values": [
        "tsne",
        "pca",
        "NULL"
      ],
      "description": "Method choosed for dimension reduction."
    },
    {
      "kind": "DoubleProperty",
      "name": "outDim",
      "defaultValue": 2,
      "values": {},
      "description": "Dimension reduction output."
    },
    {
      "kind": "DoubleProperty",
      "name": "seed",
      "defaultValue": -1,
      "values": {},
      "description": "Seed for random number generation (used for tSNE). if less than 0, set a Random seed"
    }
  ]
}