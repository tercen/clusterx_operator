{
  "name": "ClusterX",
  "description": "Performs fast clustering by automatic search and detection of density peaks.",
  "tags": [
    "clustering"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/clusterx_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "dimReduction",
      "defaultValue": "NULL",
      "values": [
        "tsne",
        "pca",
        "NULL"
      ],
      "description": "Method chosen for dimension reduction to perform."
    },
    {
      "kind": "DoubleProperty",
      "name": "outDim",
      "defaultValue": 2,
      "description": "Number of dimensions to return."
    },
    {
      "kind": "DoubleProperty",
      "name": "seed",
      "defaultValue": -1,
      "description": "Seed for random number generation. If less than 0, a random seed is set."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Variable",
            "type": "",
            "description": "Variable identifiers (channel, markers).",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Observation",
            "type": "",
            "description": "Observation identifiers (cells, samples, individuals).",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement values (numeric, per cell).",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [
                "Observation"
              ],
              "rColumns": [
                "Observation"
              ]
            },
            "rightRelation": {
              "kind": "TableRelation",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "cluster",
                  "type": "string"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}